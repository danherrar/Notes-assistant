CREATE TABLE users 
(
id INT NOT NULL,
name TEXT,
email TEXT NOT NULL UNIQUE,
password TEXT NOT NULL,
date DATE NOT NULL,
PRIMARY KEY (id));

CREATE TABLE notes 
(
id SERIAL NOT NULL,
user_id INT NOT NULL,
title TEXT NOT NULL,
body TEXT,
date DATE NOT NULL,
PRIMARY KEY(id),
FOREIGN KEY (user_id) REFERENCES users(id));
